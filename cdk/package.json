{
  "name": "cdk",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc --noEmit",
    "test": "jest --runInBand --detectOpenHandles",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "generate": "cdk synth --path-metadata false --version-reporting false",
    "diff:code": "cdk diff --path-metadata false --version-reporting false Prism-CODE",
    "diff:prod": "cdk diff --path-metadata false --version-reporting false Prism-PROD"
  },
  "devDependencies": {
    "@guardian/cdk": "53.0.3",
    "@guardian/eslint-config-typescript": "^9.0.2",
    "@types/jest": "^29.5.11",
    "@types/node": "20.11.10",
    "aws-cdk": "2.121.1",
    "aws-cdk-lib": "2.121.1",
    "constructs": "10.3.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}

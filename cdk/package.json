{
  "name": "cdk",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc --noEmit",
    "test": "jest --runInBand --detectOpenHandles",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "generate": "cdk synth --path-metadata false --version-reporting false",
    "diff:code": "cdk diff --path-metadata false --version-reporting false Prism-CODE",
    "diff:prod": "cdk diff --path-metadata false --version-reporting false Prism-PROD"
  },
  "devDependencies": {
    "@guardian/cdk": "48.4.0",
    "@guardian/eslint-config-typescript": "^2.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "18.11.10",
    "aws-cdk": "2.45.0",
    "aws-cdk-lib": "2.45.0",
    "constructs": "10.1.127",
    "eslint": "^8.27.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.3"
  }
}
